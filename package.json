{
  "name": "generator-terragrunt-terraform",
  "version": "1.0.1",
  "description": "A Yeoman Generator to generate a skeleton Terragrunt project and/or module",
  "homepage": "",
  "author": {
    "name": "Hung Luong",
    "email": "hungluong5791@gmail.com",
    "url": "https://github.com/hungluong5791"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "terragrunt",
    "terraform",
    "yeoman-generator"
  ],
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/yeoman-generator": "^5.2.10",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^26.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "aws-regions": "^2.3.1",
    "chalk": "^2.1.0",
    "generator-git-init": "^1.1.3",
    "mkdirp": "^1.0.4",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": 0,
      "max-nested-callbacks": [
        1,
        {
          "max": 10
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint --fix .",
    "test": "jest"
  },
  "repository": "hungluong5791/generator-terragrunt-terraform",
  "license": "MIT"
}
